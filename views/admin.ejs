<!DOCTYPE html>
<html lang="en">
<head>
    <title>Admin</title>
    <link rel="stylesheet" type="text/css" href="/stylesheets/admin.css">
</head>
<body>
   <div class="wrapper">
       <div class="header">
           <h3>Панель администратора</h3>
       </div>
       <div class="userInfo">
            <h4>Логин пользователя</h4><h4>Роль пользователя</h4>
           <ul class="userList">
               <% users.forEach(function(item){ %>
                <% if(item.rule !== "admin"){ %>
               <li class="user">
                  <p><%= item.login %></p>
                  <p><%= item.rule %></p>
                   <% } %>
               </li>
                   <% }); %>
           </ul>
       </div>
   </div>
   <script src="/javascripts/admin.js" type="text/javascript"></script>
</body>
</html>